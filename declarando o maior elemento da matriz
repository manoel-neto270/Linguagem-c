#include <stdio.h>

int main()
{
    int matriz[2] [3] = {{3,5,1} , {7,2,6}};
    int i, j;
    int maior = matriz [0][0];
    for(i =0; i<2; i++)
    {
        for(j = 0; j<3; j++)
        {
            if(matriz[i][j] > maior)
        
        
        {
            maior = matriz[i][j];
        }
        }
    }
    
    for(i = 0; i <1; i++)
    {
         for(j = 0; j<1; j++) 
    {
        printf("Maior elemento:%d\n", maior);
    }

      
    }
  
    
    return 0;
}
