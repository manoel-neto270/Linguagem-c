poligonos.h
float calcAreaRetangulo (float b, float a);
float calcPerimetroTriangulo(float a, float b,float c);
poligonos.c
#include "poligonos.h"

float calcAreaRetangulo(float b, float a)
{
    return b*a;

}

float calcPerimetroTriangulo (float a, float b, float c)
{
  return a + b + c;
}
