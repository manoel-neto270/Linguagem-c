#include <stdio.h>
int main()
{
    int linha, coluna;
    
    
    int matriz[linha] [coluna], i, j;
    for (i = 0; i<3; i++)
        for (j = 0; j<4; j++)
        {
            printf("\nEntre com o elemento [%d] [%d]=", i + 1, j + 1);
            scanf ("%d", &matriz [i] [j]);
            
        }
        for (i = 0; i<3; i++)
        {
            printf("\n |");
            for (j = 0; j<4; j++)
                printf(" %d ", matriz [i] [j]);
                printf("|");
        }
        {
            if ((i = -1) && (j = -1));
            i = 0;
            j = 0;
            
        }
        
        getchar();
        return (0);
}
