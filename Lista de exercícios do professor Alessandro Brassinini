//10º questão
int main()
{
    setlocale(LC_ALL, "Portuguese");
    int paes, broas;
    float total, poupanca;
    printf("Quantidade de pães vendidos:");
    scanf("%f", &paes);
    printf("Quantidade de broas vendidas");
    scanf("%f", &broas);

    total = paes * 0.12 + broas * 1.50;
    poupanca = total * 0.10;
    printf("\nTotal arrecadado: R$ %2.f", total);
    printf("\nPoupança: R$ %2.f", poupanca);


