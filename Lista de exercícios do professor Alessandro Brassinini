//11º questão
int main (){
    setlocale( LC_ALL, "Portuguese");
    float nota1, nota2, nota3, media;
    printf("Digite a nota 1:");
    scanf("%f", &nota1);
    printf("Digite a nota 2:");
    scanf("%f", &nota2);
    printf("Digite a nota 3:");
    scanf("%f", &nota3);
    media = (nota1 + nota2 + nota3 * 3) / 6;
    printf("\nSua média é de %f\n", media);
}

#include <stdio.h>
