#include <stdio.h>

int main()
{
    int matriz[2] [2]= {{2,4} , {6, 8}};
    int i, j, soma =0;
    float media;
    for(i = 0;i<2; i++)
    {
        for(j = 0; j<2; j++)
        
    
    {
     soma += matriz [i] [j];
    }
    }
    media = soma /4.0;
    printf("Media %.2f\n", media);
    return 0;
}
