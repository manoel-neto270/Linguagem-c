//22º questão
int main (){
    setlocale( LC_ALL, "Portuguese");
    float nota1, nota2, media;
    printf("Entre com as notas"),
    scanf("%f %f", &nota1, &nota2);
    media = (nota1 * 2 + nota2 * 3) / (2+3);
    printf("\nMédia é: %f\n", media);
    return 0;




}

