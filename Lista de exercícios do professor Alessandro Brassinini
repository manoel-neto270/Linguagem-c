#include <stdio.h>
#include <locale.h>
//24º questão
int main (){
    setlocale( LC_ALL, "Portuguese");
    float base_maior, base_menor, altura, area;
    printf("Entre com a medida da base menor do trapézio");
    scanf("%f", &base_menor);
    printf("Entre com a medida da base maior do trapézio");
    scanf("%f", &base_maior);
    printf("Entre com a medida da altura do trapézio");
    scanf("%f", &altura);
    area = (base_menor + base_maior) * altura / 2;
    printf("\nA área do trapézio é: %f\n", area);
    return 0;


}

#include <stdio.h>
#include <locale.h>
#include <math.h>
//25º questão
int main (){
    setlocale( LC_ALL, "Portuguese");
    float catB, catC, hip;
    printf("Entre com a medida do primeiro cateto:"),
    scanf("%f", &catB);
    printf("Entre com a medida do segundo cateto");
    scanf("%f", &catC);
    hip = sqrt(catB * catB + catC * catC);
    printf("\nA medida da hipotenusa é %2.f\n", hip);
    return 0;



}



