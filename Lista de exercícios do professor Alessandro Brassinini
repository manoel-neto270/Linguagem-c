//21º questão
int main (){
    setlocale( LC_ALL, "Portuguese");
    int n1, n2, n3, total;
    printf("Entre com os três números inteiros");
    scanf("%d %d %d", &n1, &n2, &n3);
    total = n1 * n2 * n3;
    printf("\nO produto dos três números é: %d\n", total);
    return 0;


}

