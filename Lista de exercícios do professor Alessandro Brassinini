#include <stdio.h>
#include <locale.h>
//19º questão
int main (){
    setlocale( LC_ALL, "Portuguese");
    float alt_predio, som_predio, alt_pessoa, som_pessoa;
    printf("Qual a sua altura?");
    scanf("%f", &alt_pessoa);
    printf("Quanto mede a sua sombra");
    scanf("%f", &som_pessoa );
    printf("Quanto mede a sombra do prédio");
    scanf("%f", &som_predio);
    alt_predio = alt_pessoa * som_predio / som_pessoa;
    printf("\nA altura do prédio mede %2.f metros\n", alt_predio);
    return 0;


}

#include <stdio.h>
#include <locale.h>
#define PI 3.1415
//20º questão
int main (){
    setlocale( LC_ALL, "Portuguese");
    float raio, altura, area, volume;
    printf("Entre com o raio do cilindro"),
    scanf("%f", &raio);
    printf("Entre com a altura do cilindro");
    scanf("%f", &altura);
    area = PI * raio * raio;
    volume = area * altura;
    printf("\nO volume deste cilindro é: %f\n", volume);
    return 0;

}
