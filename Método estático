#include"stdio.h"//função printf()
//Criando um método estatico
void meumetodoestatico()
{
        // Cria uma variável novamente
        int static minhaidade = 18;
        minhaidade++;
        printf("%d \n", minhaidade);
        // Por ser static não vai ser eliminada da memória
}


int main()
{
    meumetodoestatico();
    meumetodoestatico();
    meumetodoestatico();
    return 0;
}
